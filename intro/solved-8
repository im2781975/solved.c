/*124
62 60 58 56 54 52 50 48 46 44 42 40 38 36 34 32 30 28 26 24 22 20 18 16 14 12 10 8 6 4 2 0 -1 */
#include<stdio.h>
int main()
{
    int i, n;
    scanf("%d",&n);
    for(i=n;i>=1;i--)
    {
        if(i%2==0)
        i/=2;
        else
        i-=1;
        printf("%d ",i);
    }
    printf("%d ",i);
}
