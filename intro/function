#include <stdio.h>
int x,y;
//without arg without return values
void sub_4()
{
  int p=4,q=3;
 printf("p-q = %d\n",p-q);
}
//without arg with return values
int sub_3() 
{
  int p=2,q=6;
  return(p+q);
}
//with arg without return values
void sub_2(int a,int b)
{
  int res=x-y;
 printf("x-y = %d\n",res);
}
// func with arg & return values.
int sub_1(int a,int b) 
{
  return(a-b);  
}
int main()
{
int x=5,y=2;
  printf("x-y = %d\n",sub_1(x,y));
  sub_2(x,y);
  printf("%d\n",sub_3());
  sub_4();
}
