/* print the according to the order
0-32=F |33-39=D |40-49=C |50-59=B |60-69=A- |70-79=A
80-100=A+

->65
<-A-   */
#include<stdio.h>
int main()
{
    int n;
    scanf("%d", &n);
    if(n>=0 && n<=32)
        printf("F\n");
    else if(n>=33 && n<=39)
        printf("D\n");
    else if(n>=40 && n<=49)
        printf("C\n");
    else if(n>=50 && n<=59)
        printf("B\n");
    else if(n>=60 && n<=69)
        printf("A-\n");
    else if(n>=70 && n<=79)
        printf("A\n");
    else if(n>=80)
        printf("A+\n");

    return 0;
}
