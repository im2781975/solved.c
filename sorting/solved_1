/* sort the first array in ascending & second array in discending order.print the diffrence
-> 5
2 3 1 4 5
1 5 3 7 8
<- -9 -6 -1 2 5 8 */
#include <stdio.h>
int main() {
    int n;

    scanf("%d",&n);

    int a[n],b[n];

    for(int i=0; i<n; i++)

        scanf("%d",&a[i]);

    for(int i=0; i<n; i++)

        scanf("%d",&b[i]);


    int min,max;


    for(int i=0; i<n; i++)

    {

        for(int j=i+1; j<n; j++)

        {

            if(a[j]<a[i])

            {

                min = a[i];

                a[i] = a[j];

                a[j] = min;

            }

        }

    }


    for(int i=0; i<n; i++)

    {

        for(int j=i+1; j<n; j++)

        {

            if(b[j]>b[i])

            {

                max = b[i];

                b[i] = b[j];

                b[j] = max;

            }

        }

    }


    for(int i=0; i<n; i++)

        printf("%d ",(a[i]-b[i]));

    return 0;

}
