int main()
{
    int a, b , c;
     while(scanf("%d %d", &a, &b)!=EOF)
     {
     if (a<0 && b<0)
     break;
        else{
     c= abs(a-b);
    if (c>50)
    c= 100-c;
    printf("%d\n", c);
    }
    }
    return 0;
    }
