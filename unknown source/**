#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,a[5],b[5],n,m;
    cin>>t;
    for(int j=1;j<=t;j++)
    {

        for(int i=0;i<3;i++)
        {
            cin>>a[i];
            b[i]= a[i];
        }
        sort(a,a+3);
        cout<<"Data set #"<<j<<":"<<endl;
        cout<<"Orijinal order: ";
        for(int i=0;i<3;i++)
            cout<<b[i]<<" ";
        cout<<endl;
        cout<<"Smallest to Largest: ";
            for(int i=0;i<3;i++)
                cout<<a[i]<<" ";
        cout<<endl;
       cout<<endl;
    }
    return 0;
}
#include<stdio.h>

int main()
{
  int num1,num2;

  printf("Enter any two numbers\n");
  scanf("%d%d",&num1,&num2);

  num1 = num1 + num2;
  num2 = num1 - num2;
  num1 = num1 - num2;

  printf("After swapping,the numbers are %d,%d\n",num1,num2);

  return 0;
}
#include<stdio.h>
int main()
{
    char country[] = {'B', 'A', 'N', 'G', 'L', 'A', 'D', 'E', 'S', 'H'};
    int i, length;
        printf("%s\n", country);
        length=10;
        for(i=0; i<length; i++) {
            if(country[i]>=65 && country[i]<=90) {
                country[i]= 'a'+ (country[i]- 'A');
                }
        }
        printf("%s\n", country);
        return 0;
}
#include<stdio.h>
int main()
{
    char country[] = {'B', 'a', 'n', 'g', 'l', 'a', 'd', 'e', 's', 'h'};
    int i, length;
        printf("%s\n", country);
        length=10;
        for(i=0; i<length; i++) {
            if(country[i]>=97 && country[i]<=122) {
                country[i]= 'A'+ (country[i]- 'a');
                }
        }
        printf("%s\n", country);
        return 0;
}
//standard deviation.
#include<stdio.h>
#include<math.h>
#define Max 10
int main()
{
    float x[Max], avrg, var, sd, sum=0, sum1=0;
    int i, n;
    printf ("Enter the value of N\n");
    scanf("%d", &n);
    printf("Enter %d real number\n",n);
    for(i=0; i<n; i++){
        scanf("%f", &x[i]);
        }
     for(i=0; i<n; i++){
         sum=sum+x[i];
         }
         avrg=sum/n;
         for(i=0; i<n; i++){
             sum1= sum1+ pow(x[i]-avrg, 2);
             }
             var=sum1/n;
             sd=sqrt(var);
             printf("standard deviation= %0.2f\n", sd);
             return 0;
             }
// cgpa
#include<stdio.h>
int main()
{
    char name[50];
    int a[5], G[5], i;
    float cgpa;

    printf("Enter Student Name:");
    scanf("%s", &name);
    printf("Enter his course marks: ");
        for(i=0; i<5 ;i++)
    scanf("%d", &a[i]);

        for(i=0; i<5; i++)
    if(a[i]>=80 && a[i]<=100)
        G[i]=4;
        else if(a[i]>=70 && a[i]<= 79)
            G[i]= 3.5;
            else if(a[i]>=60 && a[i]<= 69)
                G[i]= 3;
                else if(a[i]>=50 && a[i]<= 59)
                    G[i]= 2.5;
                        else if(a[i]<=49)
                                G[i]= 0;
                                 cgpa= ((( G[0]*3)+(G[1]*1.5)+(G[2]*3)+(G[3]*1.5)+(G[4]*2))/ (3+1.5+3+1.5+2));
                                 printf("cgpa of %s is : %0.2f \n", name , cgpa);

                                 if (cgpa== 0)
                                     printf(" Letter Grade: F");
                                         else if (cgpa<= 2.5)
                                     printf("Letter Grade : D");

                                        else if (cgpa<= 3)
                                     printf(" Letter Grade: C");
                                         else if (cgpa<= 3.5)
                                     printf("Letter Grade : B");

                                        else if (cgpa<= 4)
                                     printf(" Letter Grade : A");
                                     return 0;
                                     }

