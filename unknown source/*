#include <stdio.h>
int main ()
{
    int n = 5;
    int i = 1;

    while  (i<= 10) {
        printf ("%d * %d = %d\n", n , i,
n*i);
    i = i + 1;
 }
     return 0;
}
//prime number
#include<stdio.h>
#include<conio.h>
int main()
{
    int num,i,count,n;
    printf("Enter max range:");
    scanf("%d",&n);
    for(num=1;num<=n;num++){

        count=0;

    for (i=2;i<=num/2; i++){
        if(num%i==0){
        count++;
        break;
        }
    }
    if(count==0 && num!=1)
        printf("%d\n",num);
    }
    return 0;
    }
//prime words
#include<stdio.h>
#include<conio.h>
int main()
{
    int i=2, j, p;
    while(i<=100){

    p=1;
    for(j=2; j<i; j++){
        if(i%j==0){
            p=0;
            }
        }
        if(p){
        printf("%d\n",i);
        }
    i++;
    }
    return 0;
    }
//prime number
#include<stdio.h>
#include<conio.h>
int main()
{
    int n, c;
    printf("Enter a number\n");
    scanf("%d",&n);

    for(c=2; c<=n-1; c++)
    {
        if(n%c==0)
        {
        printf("%d is not prime\n",n);
        break;
        }
            }
            if(c==n)
                printf("%d is prime\n",n);
                return 0;
                   }
// sort

#include<bits/stdc++.h>
using namespace std;
int main()
{
     int a[100],i,j,t, n;
     scanf("%d", &t);
     for(j=0; j<t; j++)
     {
     scanf("%d", &n);
     for(i=0; i<3; i++)
     {
          scanf("%d", &a[i]);

     }
          sort(a,a+3);


     for(i=0; i<3; i++){
          printf("Data set #%d:\n Orginal order:\n smallest to largest: a[i]\n");
     }

}
 return 0;
}
//goto
#include<stdio.h>
int main()
{
    int x='A';
    start:
        printf("%c=%d\n", x,x);
        x++;
        goto start;
            return 0;
    }

