/*5
2 4 6 8 9
9 8 6 4 2 */
#include<stdio.h>
int i,j;
int rev_arr(int n,int arr[])
{
    for(i=0,j=n-1;i<n/2;i++,j--)
    {
        int tmp=arr[i];
        arr[i]=arr[j];
        arr[j]=tmp;
    }
    for(i=0;i<n;i++)
    printf("%d ",arr[i]);
}
int arr_rev(int n,int arr[])
{
    int rev_arr[n+1];
    for(i=n-1,j=0;i>=0;i--,j++)
    {
        rev_arr[j]=arr[i];
    }
        
    for(i=0;i<n;i++)
    {
    printf("%d ",rev_arr[i]);
    }
}
int main()
{
    int i,j,n;
    scanf("%d",&n);
    int arr[n+4];
    for(i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    //rev_arr(n,arr);
    arr_rev(n,arr);
}
