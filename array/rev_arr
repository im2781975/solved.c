/*5
7 8 9 6 5
5 6 9 8 7 */
#include<stdio.h>
int main()
{
    int i,j,n,arr[n+1];
    scanf("%d",&n);
    for(i=0;i<n;i++)
     scanf("%d",&arr[i]);
     
    int rev_arr[n+1];
    for(i=n-1,j=0;i>=0;i--,j++)
    {
        rev_arr[j]=arr[i];
    }
        
    for(i=0;i<n;i++)
    {
    printf("%d ",rev_arr[i]);
    }
}
