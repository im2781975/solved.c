/*5
2 4 6 8 9
9 8 6 4 2 */
#include<stdio.h>
int main()
{
    int i,j,n;
    scanf("%d",&n);
    int arr[n+4];
    for(i=0;i<n;i++)
    {
    scanf("%d",&arr[i]);
    }
    for(i=0,j=n-1;i<n/2;i++,j--)
    {
        int tmp=arr[i];
        arr[i]=arr[j];
        arr[j]=tmp;
    }
    for(i=0;i<n;i++)
    printf("%d ",arr[i]);
}
