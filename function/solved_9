#include<stdio.h>
int add_two_numbs(int num1,int num2)
{
    int sum=num1+num2;
    return sum;
}
int print_sum(int num)
{
    printf("%d\n",num);
}
float get_pi(void)
{
    return 3.1416;
}
int main()
{
    int a=2,b=5;
    int sum_ab=add_two_numbs(a,b);
    print_sum(sum_ab);
    float pi=get_pi();
    printf("%f\n",pi);
    
}
