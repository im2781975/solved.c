
/* 
3 -3
power is 0.037037
sqrt is 1.732051
absolute value is: 3
sin  value is: 0.866027
*/
#include<stdio.h>
#include<math.h>
int main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    printf("power is %f\n",pow(a,b));
    printf("sqrt is %f\n",sqrt(a));
    printf("absolute value is: %d\n",abs(b));
    printf("sin  value is: %f",sin(3.1416/a));
}
