/*45 78 34
max is:78
min is:34 */
#include<stdio.h>
int max(int a,int b,int c)
{
    if(a>b && a>c)
       return a;
       
    else if(b>a && b>c)
       return b;
       
    else 
        return c;
}
int min(int a,int b,int c)
{
    if(a<b && a<c)
       return a;
       
    else if(b<a && b<c)
       return b;
       
    else 
        return c;
}
int main()
{
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    int m=max(a,b,c);
    printf("max is:%d\n",m);
    int n=min(a,b,c);
    printf("min is:%d\n",n);
    
}
