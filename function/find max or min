/*2 
57 87 34
max is:87
min is:34
67 45 98
max is:98
min is:45
*/
#include<stdio.h>
int max(int a,int b,int c)
{
    if(a>b && a>c)
       return a;
       
    else if(b>a && b>c)
       return b;
       
    else 
        return c;
}
int min(int a,int b,int c)
{
    if(a<b && a<c)
       return a;
       
    else if(b<a && b<c)
       return b;
       
    else 
        return c;
}
int main()
{
    int a,b,c,i,n;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
    scanf("%d%d%d",&a,&b,&c);
    int m=max(a,b,c);
    printf("max is:%d\n",m);
    int n=min(a,b,c);
    printf("min is:%d\n",n);
    }
    
}
