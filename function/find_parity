//Even
#include<stdio.h>
int is_Even(int n)
{
    if(n%2==0)
    return 1;
    else 
    return 0;
}
void find_parity(int n)
{
    if(n%2==0)
    printf("Even\n");
    else
    printf("odd\n");
    
    return;
}
int main()
{
    int n;
    scanf("%d",&n);
  //)0  find_parity(n);
    if(is_Even(n))
    printf("Even\n");
    else
    printf("odd\n");
    
    return 0;
}
