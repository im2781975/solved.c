/* ->12 18
GCD is:6
LCM is:36*/
#include<stdio.h>
int i,ans;
int gcd(int a,int b)
{
     for(i=a;i>=1;i--)
    {
        if(a%i==0 && b%i==0)
        {
            ans=i;
            break;
        }
    }
    return i;
}
int lcm(int a,int b)
{
     for(i=a;;i++)
    {
        if(i%a==0 && i%b==0)
        {
            ans=i;
            break;
        }
    }   
    return i;
}
int main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    int ans=gcd(a,b);
    int ans2=lcm(a,b);
    printf("GCD is:%d\n",ans);
    printf("LCM is:%d\n",ans2);
}
