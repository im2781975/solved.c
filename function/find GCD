/* ->12 18
<-GCD is:6 */
#include<stdio.h>
int i,ans;
int gcd(int a,int b)
{
     for(i=a;i>=1;i--)
    {
        if(a%i==0 && b%i==0)
        {
            ans=i;
            break;
          //return i;
        }
    }
}
int main()
{
    int a,b;
    scanf("%d%d",&a,&b);
    int ans=gcd(a,b);
    printf("GCD is:%d",ans);
}
