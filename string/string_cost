/*bbca
Yes
cost->2^3
*/
#include<stdio.h>
int ans=0;
int check_power(int count)
{
    while(count!=1)
    {
        int rem=count%2;
        count/=2;
        if(rem!=0)
        {
            ans=0;
            break;
        }ans++;
    }
    return ans;
}
int main()
{
    char s[100];
    scanf("%s",s);
    int count=0;
    for(int i=0;s[i]!='\0';i++)
    {
       // printf("%c->%d\n",s[i],s[i]-'a'+1);
       count+=s[i]-'a'+1;
    }
    //printf("%d ",check_power(count));
    int ans=check_power(count);
    if(ans!=0)
    {
        printf("Yes\n");
        printf("cost->2^%d\n",ans);
    }
    else
    printf("No\n");
}
