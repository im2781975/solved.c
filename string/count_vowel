/*ibrahim molla
5 */
#include<stdio.h>
int main()
{
    char ch[25];
    int i=0,count_vowel=0;
    fgets(ch,sizeof(ch),stdin);
    while(ch[i]!='\0')
    {
        if(ch[i]=='a'||ch[i]=='e'||ch[i]=='i'||ch[i]=='o'||ch[i]=='u')
        count_vowel++;
        i++;
    }
    
    printf("v->%d",count_vowel);
}
