#include<stdio.h>
int i,j;
int get_string(char s[])
{
    int n=0;
    for(int i=0;s[i]!='\0';i++)
    {
        n++;
    }
    return n;
}
void string_rev(char s[])
{
    int len=get_string(s);
    for(i=0,j=len-1;i<len/2;i++,j--)
    {
        char tmp=s[i];
        s[i]=s[j];
        s[j]=tmp;
    }
}
void solve()
{
    char s[100];
    scanf("%s",s);
    string_rev(s);
    printf("%s\n",s);
}
int main()
{
   int t;
   scanf("%d",&t);
   while(t--)
   {
       solve();
   }
}
