
/*print the grading policy

Mark Range
Grade
80-100
A
60-79
B
40-59
C
0-39
F */
#include<stdio.h>
char getMyGrade(int marks)
{
    if(marks>=0 && marks<=39)
        return 'F';
    else if(marks>=40 && marks<=59)
        return 'C';
    else if(marks>=60 && marks<=79)
        return 'B';
    else
        return 'A';
}
int main()
{
    int marks_;
    scanf("%d",&marks_);
    printf("Grade = %c \n",getMyGrade(marks_));

    return 0;
}
