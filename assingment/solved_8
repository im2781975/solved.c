
/* take a 3x3 matrix as input & print the transpose of that matrix. */
#include<stdio.h>
int main()
{
    int A[3][3],transpose[3][3],i,j,row=3,col=3;


    //Scanning A matrix
    for(i=0; i<row; i++)
    {
        for(j=0; j<col; j++)
        {
            printf("A[%d][%d] =",i,j);
            scanf("%d",&A[i][j]);
        }
        printf("\n");
    }


    //Transpose Matrix
    for(i=0; i<row; i++)
    {
        for(j=0; j<col; j++)
        {
            transpose[j][i]=A[i][j];
        }
    }

    //Printing transpose matrix
    printf("Transpose - \n");
    for(i=0; i<col; i++)
    {
        printf("\t");
        for(j=0; j<row; j++)
        {
            printf("%d ",transpose[i][j]);
        }
        printf("\n");
    }
    return 0;
}
